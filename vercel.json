{
    "version": 2,
    "installCommand": "npm install",
    "outputDirectory": ".vercel/output",
    "functions": {
        "api/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "redirects": [
        {
          "source": "/(.*)",
          "destination": "/api/base"
        }
    ],
    "cleanUrls": true
  }